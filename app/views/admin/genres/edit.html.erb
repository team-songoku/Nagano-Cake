  <div class="container">
    <div class="row">
      <div class="col">
        <h1>ジャンル編集</h1>
          <%= form_with model: @genre, url: admin_genre_path,local: true do |f| %>
            <div class="form-group">
              <%= f.label :name,"ジャンル名" %>
              <%= f.text_field :name %>
              <%= f.submit "変更を登録", class: 'btn btn-outline-secondary' %>
            </div>
          <% end %>
      </div>
    </div>
  </div>
